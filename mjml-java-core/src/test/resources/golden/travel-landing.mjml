<mjml>
  <mj-head>
    <mj-attributes>
      <mj-all padding="0px"></mj-all>
      <mj-class
        name="preheader"
        color="#000000"
        font-size="11px"
        font-family="Ubuntu, Helvetica, Arial, sans-serif"
        padding="0 20px"
      ></mj-class>
      <mj-class
        name="cta"
        color="#FFFFFF"
        font-size="13px"
        border-radius="3px"
        href="https://example.com"
        font-family="Ubuntu, Helvetica, Arial, sans-serif"
        padding="20px 25px"
      ></mj-class>
      <mj-class
        name="footer-text"
        align="center"
        color="#ffffff"
        font-family="Ubuntu, Helvetica, Arial, sans-serif"
        font-size="15px"
        padding="0 25px"
      ></mj-class>
      <mj-navbar-link
        padding="0 35px"
        font-weight="bold"
        font-size="12px"
      ></mj-navbar-link>
    </mj-attributes>
    <mj-style inline="inline"
      >a { text-decoration: none; color: inherit; }</mj-style
    >
  </mj-head>
  <mj-body background-color="#d2eeff">
    <mj-section padding-bottom="20px" padding-top="20px">
      <mj-group>
        <mj-column>
          <mj-text mj-class="preheader"
            ><a href="https://example.com">Summer Travel Deals Inside!</a></mj-text
          >
        </mj-column>
        <mj-column>
          <mj-text mj-class="preheader" align="right"
            ><a href="https://example.com/view">View in browser</a></mj-text
          >
        </mj-column>
      </mj-group>
    </mj-section>
    <mj-section background-color="#ffffff">
      <mj-column>
        <mj-image
          src="https://via.placeholder.com/105x50/ffffff/333?text=Worldly"
          alt="Worldly logo"
          align="center"
          width="105px"
          padding="10px 0"
        ></mj-image>
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffe118" padding="20px">
      <mj-column>
        <mj-navbar
          base-url="https://example.com"
          hamburger="hamburger"
          ico-color="#444"
        >
          <mj-navbar-link href="/home">home</mj-navbar-link>
          <mj-navbar-link href="/deals">Summer deals</mj-navbar-link>
          <mj-navbar-link href="/blog">Our blog</mj-navbar-link>
          <mj-navbar-link href="/follow">Follow us</mj-navbar-link>
        </mj-navbar>
      </mj-column>
    </mj-section>
    <mj-section
      background-url="https://via.placeholder.com/600x400/2196F3/fff?text=Ocean+View"
      background-repeat="no-repeat"
      background-size="cover"
      padding="20px 0"
    >
      <mj-column>
        <mj-text
          align="center"
          color="#ffffff"
          line-height="32px"
          font-family="Ubuntu, Helvetica, Arial, sans-serif"
          font-size="28px"
          padding="10px"
          >ARE YOU <br />
          SEASCAPE?</mj-text
        >
      </mj-column>
      <mj-column width="50%" vertical-align="bottom">
        <mj-text
          align="center"
          color="#ffffff"
          line-height="32px"
          font-family="Ubuntu, Helvetica, Arial, sans-serif"
          font-size="28px"
          padding-top="180px"
          >OR <br />
          CITYSCAPE?</mj-text
        >
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding="20px 0">
      <mj-column>
        <mj-text
          align="center"
          font-family="Ubuntu, Helvetica, Arial, sans-serif"
          font-size="15px"
          padding="10px 25px"
          color="#BD8714"
        >
          <p>SUNNIEST DESTINATIONS</p>
        </mj-text>
        <mj-image
          src="https://via.placeholder.com/240x160/FFD700/333?text=Maldives"
          alt="Maldives, Corse : -15%"
          width="240px"
        ></mj-image>
        <mj-button mj-class="cta" background-color="#bd8714"
          >BOOK NOW</mj-button
        >
        <mj-image
          src="https://via.placeholder.com/240x160/00BCD4/fff?text=Tahiti"
          alt="Tahiti, Bora Bora : -10 %"
          width="240px"
          padding="10px 25px"
        ></mj-image>
        <mj-button mj-class="cta" background-color="#bd8714"
          >BOOK NOW</mj-button
        >
      </mj-column>
      <mj-column>
        <mj-text
          align="center"
          font-family="Ubuntu, Helvetica, Arial, sans-serif"
          font-size="15px"
          padding="10px 25px"
          color="#BD8714"
        >
          <p>CHEAP CITY TOUR</p>
        </mj-text>
        <mj-image
          src="https://via.placeholder.com/240x160/9C27B0/fff?text=Stockholm"
          alt="Stockholm, Rome : from 162 EUR / day"
          width="240px"
        ></mj-image>
        <mj-button mj-class="cta" background-color="#48b6bf"
          >BOOK NOW</mj-button
        >
        <mj-image
          src="https://via.placeholder.com/240x160/607D8B/fff?text=New+York"
          alt="New York, London : -30 %"
          width="240px"
          padding="10px 25px"
        ></mj-image>
        <mj-button mj-class="cta" background-color="#48b6bf"
          >BOOK NOW</mj-button
        >
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding="20px 0">
      <mj-column>
        <mj-image
          href="https://example.com/guide"
          src="https://via.placeholder.com/250x100/4CAF50/fff?text=City+Guide"
          alt="Download your city guide"
          width="250px"
          padding="0 25px"
        ></mj-image>
      </mj-column>
      <mj-column>
        <mj-text
          align="center"
          color="#000000"
          font-family="Ubuntu, Helvetica, Arial, sans-serif"
          font-size="13px"
          padding="0 25px"
          font-weight="bold"
        >
          <p>RATE YOUR STAY !</p>
        </mj-text>
        <mj-image
          href="https://example.com/rate"
          src="https://via.placeholder.com/131x40/FF9800/fff?text=Rate+Us"
          alt="Rate your stay"
          width="131px"
        ></mj-image>
      </mj-column>
    </mj-section>
    <mj-section background-color="#4b525c">
      <mj-column width="33%" vertical-align="top">
        <mj-image
          src="https://via.placeholder.com/30x30/4b525c/fff?text=F"
          alt="Best companies for your flights"
          width="30px"
          padding="10px 25px"
        ></mj-image>
        <mj-text mj-class="footer-text">
          <p>BEST COMPANIES FOR YOUR FLIGHTS</p>
        </mj-text>
      </mj-column>
      <mj-column width="33%">
        <mj-image
          src="https://via.placeholder.com/29x29/4b525c/fff?text=I"
          alt="Insurances included"
          width="29px"
          padding="10px 25px"
        ></mj-image>
        <mj-text mj-class="footer-text">
          <p>INSURANCES INCLUDED</p>
        </mj-text>
      </mj-column>
      <mj-column width="33%" vertical-align="top">
        <mj-image
          src="https://via.placeholder.com/31x31/4b525c/fff?text=H"
          alt="Hotels best selection"
          width="31px"
          padding="10px 25px"
        ></mj-image>
        <mj-text mj-class="footer-text">
          <p>HOTELS BEST SELECTION</p>
        </mj-text>
      </mj-column>
    </mj-section>
    <mj-section background-color="#4395b1">
      <mj-column width="33.33333333333333%">
        <mj-image
          src="https://via.placeholder.com/97x40/4395b1/fff?text=Call+Us"
          alt="Call us : 0 800 123 456"
          width="97px"
          padding="10px 25px"
        ></mj-image>
      </mj-column>
      <mj-column width="33.33333333333333%">
        <mj-image
          href="https://example.com/agencies"
          src="https://via.placeholder.com/117x40/4395b1/fff?text=Find+Agency"
          alt="Meet us : find an agency"
          width="117px"
          padding="10px 25px"
        ></mj-image>
      </mj-column>
      <mj-column width="33.33333333333333%">
        <mj-image
          href="https://example.com/experts"
          src="https://via.placeholder.com/110x40/4395b1/fff?text=Ask+Expert"
          alt="Ask us : find an expert"
          width="110px"
          padding="10px 25px"
        ></mj-image>
      </mj-column>
    </mj-section>
    <mj-section padding="20px 0">
      <mj-column>
        <mj-text align="center" color="#000000" font-size="11px"
          >You are receiving this email because you subscribed to our newsletter.
          <p>123 Travel Street, Paris 75001, France</p>
        </mj-text>
      </mj-column>
    </mj-section>
  </mj-body>
</mjml>
